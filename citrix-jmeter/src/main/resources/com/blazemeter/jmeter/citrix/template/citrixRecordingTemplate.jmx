              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">//Setting storefront as default
vars.put(&quot;login_method&quot;, &quot;0&quot;);

if (vars.get(&quot;citrix_portal_context_path&quot;).contains(&quot;/vpn/index.html&quot;)) {
	//We did locate the Gateway API. Therefore, we will use Gateway 
	vars.put(&quot;login_method&quot;, &quot;1&quot;);
}

//Based on the scheme set citrix_use_https flag to No or Yes
def scheme = vars.get(&quot;citrix_portal_scheme&quot;);
vars.put(&quot;citrix_use_https&quot;, &quot;No&quot;);
if (scheme == &quot;https&quot;) {
	vars.put(&quot;citrix_use_https&quot;, &quot;Yes&quot;);
}

//When the port is defined, the url is built with that data
def port = vars.get(&quot;citrix_portal_port&quot;);

//Define Citrix Base URL
vars.put(&quot;citrix_base_url&quot;, vars[&quot;citrix_portal_scheme&quot;] + &quot;://&quot; + vars[&quot;citrix_portal_host&quot;] + (port == &quot;&quot; ? &quot;&quot; : &quot;:&quot; + port) + vars[&quot;citrix_portal_context_path&quot;]);
log.info(&quot;Variables initialized&quot;);</stringProp>
