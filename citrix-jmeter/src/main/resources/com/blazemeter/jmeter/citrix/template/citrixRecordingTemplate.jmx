                  <stringProp name="script">import java.util.regex.Matcher;
import java.util.regex.Pattern;

String response = prev.getResponseDataAsString();
def pattern = ~&quot;URL=([0-9a-zA-Z/_-]*)\&quot;&quot;;
	
Matcher matcher = pattern.matcher(response);

//Validate the Response provides a Storefront URL
if (!matcher.find()) {
	AssertionResult.setFailure(true)
	AssertionResult.setFailureMessage(&quot;Set client Storefront URL not found&quot;);
	return;
}

vars.put(&quot;citrix_base_url&quot;, matcher.group(1));	

